    "url": "https://github.com/LQ-Sebastien/kanban-personal/issues"
  },
  "homepage": "https://github.com/LQ-Sebastien/kanban-personal#readme",
  "dependencies": {
    "browserify": "^17.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "express-rate-limit": "^6.4.0",
    "frisby": "^2.1.3",
    "jest": "^28.1.0",
    "pg": "^8.7.3",
    "sanitize-html": "^2.7.0",
    "sequelize": "^6.19.0",
