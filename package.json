{
  "name": "okanban-personal",
  "version": "1.0.0",
  "description": "O'Kanban de Sebastien",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "dev": "nodemon index.js",
    "build": "browserify -e client/app.js -o public/js/bundle.js",
    "start": "node index.js",
    "db:create": "psql -U okanban -d okanban -f ./data/create_tables.sql",
    "db:seed": "psql -U okanban -d okanban -f ./data/seed_database.sql",
    "db:reset": "npm run db:create && npm run db:seed",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/LQ-Sebastien/kanban-personal"
  },
